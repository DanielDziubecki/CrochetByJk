@model CrochetByJk.ViewModel.ProductsInCategoryViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.React("CategoryProducts", new
{
    products = Model.Products,
    categoryName = Model.CategoryName
})


@section scripts
{
    <script src="~/Content/ReactComponents/CategoryProducts.jsx"></script>
    <script>AOS.init({duration: 2000});</script>
    @Html.ReactInitJavaScript()
}